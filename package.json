{
  "name": "eslint-config-kwork",
  "description": "ESLint configs for KnowledgeWork's projects.",
  "version": "1.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/knowledge-work/eslint-config-kwork.git"
  },
  "keywords": [
    "eslint",
    "config",
    "eslintconfig"
  ],
  "license": "MIT",
  "files": [
    "base.js",
    "frontend.js"
  ],
  "scripts": {
    "setup": "yarn",
    "check": "yarn run check:prettier",
    "check:prettier": "prettier --check .",
    "format": "prettier --write .",
    "preversion": "echo \"Run check for version $npm_package_version\" && yarn run check",
    "postversion": "git push --tags && git push origin main && yarn publish ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.4.17",
    "eslint-plugin-date": "0.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-jsx-expressions": "1.3.1",
    "eslint-plugin-return-type": "1.0.9",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-strict-dependencies": "1.1.0",
    "eslint-plugin-unused-imports": "3.0.0"
  },
  "devDependencies": {
    "prettier": "3.0.2",
    "prettier-config-kwork": "0.0.8"
  },
  "prettier": "prettier-config-kwork"
}
